<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Group?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #a3a1be;" stylesheets="@alphabets/CSS/sign_up.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo1.dictionary_hier">
   <children>
      <Group layoutX="241.0" layoutY="150.0">
         <children>
            <Button fx:id="alphabet1Button" mnemonicParsing="false" onAction="#onAlphabet1ButtonClick" prefHeight="60.0" prefWidth="220.0" styleClass="colorfulButton" stylesheets="@alphabets/CSS/sign_up.css" text="Alphabet 1" />
            <Button fx:id="numbersbutton" layoutX="274.0" layoutY="3.0" mnemonicParsing="false" onAction="#onNumbersButtonClick" prefHeight="60.0" prefWidth="220.0" styleClass="colorfulButton" stylesheets="@alphabets/CSS/sign_up.css" text="Numbers" />
            <Button fx:id="alphabet2Button" layoutY="93.0" mnemonicParsing="false" onAction="#onAlphabet2ButtonClick" prefHeight="60.0" prefWidth="220.0" styleClass="colorfulButton" stylesheets="@alphabets/CSS/sign_up.css" text="Alphabet 2" />
            <Button fx:id="alphabet2Button1" layoutX="274.0" layoutY="93.0" mnemonicParsing="false" onAction="#ondaysButtonClick" prefHeight="60.0" prefWidth="220.0" styleClass="colorfulButton" stylesheets="@alphabets/CSS/sign_up.css" text="Days" />
            <Button fx:id="alphabet2Button2" layoutY="181.0" mnemonicParsing="false" onAction="#onMonthsButtonClick" prefHeight="60.0" prefWidth="220.0" styleClass="colorfulButton" stylesheets="@alphabets/CSS/sign_up.css" text="Months" />
            <Button fx:id="alphabet2Button21" layoutX="274.0" layoutY="181.0" mnemonicParsing="false" onAction="#onTimeButtonClick" prefHeight="60.0" prefWidth="220.0" styleClass="colorfulButton" stylesheets="@alphabets/CSS/sign_up.css" text="Time" />
         </children>
      </Group>
      <ImageView fitHeight="50.0" fitWidth="40.0" onMouseClicked="#switchtodashboard" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Logos/back.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
